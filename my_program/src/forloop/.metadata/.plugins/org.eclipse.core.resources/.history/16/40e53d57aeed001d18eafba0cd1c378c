package forloop;

public class Basic {

	public static void main(String[] args) {

		Basic obj = new Basic();
		System.out.println("---------------------");
		obj.first();
		System.out.println("---------------------");
		obj.second();
		System.out.println("---------------------");
		obj.third();
		System.out.println("---------------------");
		obj.fourth(9);
		System.out.println("---------------------");
		obj.fivth(50);
		System.out.println("---------------------");
		obj.sixth();
		System.out.println("---------------------");
		obj.seventh();
		System.out.println("---------------------");
		obj.eighth();
	}

	public void first() {
		for (int i = 1; i <= 5; i++) {
			System.out.println(i);
		}
	}

	public void second() { // even number in 1 to 10
		for (int i = 1; i <= 10; i++) {
			if (i % 2 == 0) {
				System.out.println(i);
			}
		}
	}

	public void third() {
		for (int i = 150; i <= 200; i += 5) // 150 to 200 number in 5 divided
		{
			System.out.println(i);
		}
	}

	public void fourth(int n) {
		for (int i = 1; i <= n; i++) // 1 to 9 in device number
		{
			if (n % i == 0) {
				System.out.println(i);
			}
		}
	}
	
	public void fivth(int n)
	{
		for(int i=1;i<=n;i++) // 1 to 50 in 9 divided numbers
		{
			if(i%9==0)
			{
				System.out.println(i);
			}
		}
	}
	public void sixth()
	{
		   for(int i=100;i<=150;i++)
		   {
			   if(i%5==0&&i%3==0)  // 100 to 150 divited in 5 and 3 
			   {
				   System.out.println(i);
				   
			   }
		   }
	}
	public void seventh()
	{
		for(int i=100;i<=200;i++) // 100 and 200 divited in divides in7
		{                         // 150 above
			if(i%7==0 &&i>150)
			{
				System.out.println(i);
			}
		}
	}
	public void eighth()
	{
		for(int i=200;i>=150;i--)
		{
			if(i%4==0)
			{
				System.out.println(i);
			}
		
		}
	}
}
