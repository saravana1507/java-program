package forloop;

public class Reverseprime_forloop {

	public static void main(String[] args) {
		
	Reversenumber_forloop obj=new Reversenumber_forloop();
	obj.total(91);
	
	}
public void total(int i)
{ int reverse=0;
	for(;i>0;i/=10)
	{
		int a=i%10;
		reverse= reverse*10+a;
	}
	for(;reverse>0;reverse/=2)
	{
		
	if(reverse/2==0)
	{
		System.out.println(i+" is a not prime number");
	}
	else
	{
		System.out.println(i+" is a prime number")
	}
}
}
