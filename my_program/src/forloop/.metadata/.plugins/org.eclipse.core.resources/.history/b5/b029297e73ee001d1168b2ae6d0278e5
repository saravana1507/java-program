package inheritence;

public class food extends Foodprice {
	  public static void  main(String arg[]) {
	  food obj=new food();
	  int food= obj.food(120,35, 50);
	  int drink=obj.drinks(25, 30, 20);
	  int full=obj.total(food, drink);
	  System.out.println(" total number"+full);
	  int count=obj.discount(full);
	  System.out.println(" discount to total number"+count);
	  }
public int total(int food_total,int drinks_total)
{
	int full_total= food_total+drinks_total;
	return full_total;
	}
public int discount( int num )
{
   int avg={(num*10)/100;
   return avg;
}
}
