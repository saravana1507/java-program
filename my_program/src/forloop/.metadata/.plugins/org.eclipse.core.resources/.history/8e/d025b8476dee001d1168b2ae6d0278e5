package inheritence;

public class food extends Foodprice {

	public static void main(String[] args) {
	  food obj=new food();
	  System.out.println("------------------");
	  int food= obj.food(120, 50, 60);
	  System.out.println("food total price = "+food);
	  System.out.println("------------------");
	  int drink=obj.drinks(30, 20, 15);
	  System.out.println("drink total price = "+drink);
	}

}
