	}
	  public void number(int a,int b)
	    {
		  int i=1;
		  int highest=0;
 		  int small= a>b? b:a;
 		  int num=small>c?c:small;
 		  while(i<=small)
 		  { 
 			 if(a%i==0 && b%i==0)
 			 {
 				int  hrf=i;
 				highest=hrf;
 				 }
 		
 			 i++;	 	
 		  } 
 		 System.out.println(highest);
    	}
