package Saravana_pattern;

public class S_letter {

	public static void main(String[] args) {
		S_letter ob=new S_letter();
		ob.pattern();

	}
	public void pattern()
	{
	 for(int row=1;row<=5;row++)
	 {
		 if(row%2!=0)
		 {
			 for(int column=1;column<=5;column++)
			 {
				 System.out.print("* ");
			 }
		 }
		 if(row==2)
		 {
				 System.out.print("* ");
		
		 }
		 System.out.println();
	 }
	}
}
