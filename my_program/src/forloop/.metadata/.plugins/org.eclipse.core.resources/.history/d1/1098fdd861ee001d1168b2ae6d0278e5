package inheritence;

public class Calculator {
        
	public int addition()
	{
		int i=2;
		int j=2;
		int total=i+j;
		return total;
	}
	public int subraction()
	{
		int i=15;
		int j=7;
		int total=i-j;
		return total;
	}
	public int multipication()
	{
		int i=5;
		int j=7;
		int total=i*j;
		return total;
	}
	public int division()
	{
		int i=16;
		int j=4;
		int total=i/j;
		return total;
	}
}
